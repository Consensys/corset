WHITESPACE = _{ " " | "\t" | "\r" | "\n" }
COMMENT = _{ ";" ~ (!NEWLINE ~ ANY)* }


corset = { SOI ~ constraint* ~ EOI }

constraint = { sexpr }
sexpr = { "(" ~ function ~ expr+ ~ ")" }
expr = { column | sexpr | value }

column = @{ ASCII_ALPHA ~ (ASCII_ALPHANUMERIC | "_")* }
function = @{ (LETTER | NUMBER | SYMBOL | "-" | "*")+  }
value = @{ ASCII_DIGIT+ }
