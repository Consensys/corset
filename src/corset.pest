WHITESPACE = _{ " " | "\t" | "\r" | "\n" }
COMMENT = _{ ";" ~ (!NEWLINE ~ ANY)* }


corset = { SOI ~ constraint* ~ EOI }

constraint = { sexpr }
sexpr = { "(" ~ function ~ expr+ ~ ")" }
expr = { column | sexpr | value }

column = @{ (ASCII_ALPHA)+ }
function = @{ (ASCII_ALPHA | "-" | "+" | "=" )+ | "sub" }
value = @{ ASCII_DIGIT+ }
